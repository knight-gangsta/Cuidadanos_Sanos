@if (User.Identity.IsAuthenticated)
{
    <form method="post" class="form-inline" asp-page="/Accounts/Logout">
        <h4>Bienvenido</h4>
        @User.Identity.Name
        <p>
        <button id="logoutButton" type="submit">Cerrar Sesion</button>
        </p>
    </form>

}
else
{
    <a  asp-page="/Accounts/Login" id="login">Login</a>
    <a  asp-page="/Accounts/Register" id="Register">Register</a>



}